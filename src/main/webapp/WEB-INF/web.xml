<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	<servlet>  
    <servlet-name>Login</servlet-name>  
    <servlet-class>Login</servlet-class>  
  </servlet>  
  <servlet>  
    <servlet-name>WelcomeServlet</servlet-name>  
    <servlet-class>WelcomeServlet</servlet-class>  
  </servlet>  
  <servlet>  
    <servlet-name>ManagerServlet</servlet-name>  
    <servlet-class>ManagerServlet</servlet-class>  
  </servlet> 
  <servlet>  
    <servlet-name>EmployeeReim</servlet-name>  
    <servlet-class>EmployeeReim</servlet-class>  
  </servlet> 
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<servlet-class>RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PendingEmployee</servlet-name>
  	<servlet-class>PendingEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoadPending</servlet-name>
  	<servlet-class>LoadPending</servlet-class>
  </servlet>
  
  <servlet-mapping>  
    <servlet-name>Login</servlet-name>  
    <url-pattern>/servlet1</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>  
    <servlet-name>WelcomeServlet</servlet-name>  
    <url-pattern>/servlet2</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>  
    <servlet-name>ManagerServlet</servlet-name>  
    <url-pattern>/servlet3</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>
  	<servlet-name>EmployeeReim</servlet-name>
  	<url-pattern>/servlet4</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/servlet5</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PendingEmployee</servlet-name>
  	<url-pattern>/servlet6</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoadPending</servlet-name>
  	<url-pattern>/servlet7</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>  
   <welcome-file>index.jsp</welcome-file>  
  </welcome-file-list>  
</web-app>
